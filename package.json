{
  "name": "conversionjs",
  "version": "0.0.0",
  "description": "Ajax page transition",
  "main": "index.js",
  "scripts": {
    "watch": "npm-watch",
    "watch:dev": "node_modules/.bin/concurrently \"npm run watch\" \"npm run demo:server\"",
    "build": "npm run build:dist && npm run build:dist:min",
    "build:dist": "node_modules/.bin/rollup --config config/rollup.config.js",
    "build:dist:min": "node_modules/.bin/rollup --config config/rollup.config.min.js",
    "demo:server": "node_modules/.bin/browser-sync start -s 'demo' --serveStatic 'dist' -f 'demo/*.*' 'dist/conversion.js'"
  },
  "watch": {
    "build": "src/**/*.js"
  },
  "keywords": [
    "conversionjs",
    "ajax",
    "transition"
  ],
  "author": "Vitaliy Bykovets",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.26.3",
    "concurrently": "^4.0.1",
    "npm-watch": "^0.4.0",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-uglify": "^6.0.0"
  }
}
